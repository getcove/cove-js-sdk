{
  "name": "cove-js-sdk",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "test": "pnpm -r test && vitest run",
    "test:watch": "pnpm -r test:watch && vitest watch",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "check": "pnpm typecheck && pnpm lint",
    "check:fix": "pnpm typecheck && pnpm lint:fix",
    "clean": "pnpm -r clean && rm -rf dist node_modules *.tsbuildinfo",
    "changeset": "changeset",
    "version": "changeset version && pnpm install --no-frozen-lockfile",
    "release": "pnpm check && pnpm build && changeset publish",
    "audit": "pnpm audit --audit-level moderate",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.6",
    "@types/node": "^24.3.0",
    "@vitest/ui": "^3.2.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
